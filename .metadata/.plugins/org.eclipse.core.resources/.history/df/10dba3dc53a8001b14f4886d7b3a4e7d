package oopIntro;

public class Product {
	int id;
	String name;
	double unitPrice;
	String detail;
	double discount;
	double unitPricePriceAfterDiscount;
	
	
	public Product() {
		
	}
	
	public Product(int id, String name, double unitPrice, String detail, double discount, double unitPricePriceAfterDiscount) {
		super();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
		this.discount = discount;
		this.unitPricePriceAfterDiscount = unitPricePriceAfterDiscount;
	}
}
